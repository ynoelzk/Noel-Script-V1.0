    changeButton.Position = UDim2.new(0.1, 0, 0.6, 0)
    changeButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
    changeButton.Text = "CLIQUE PARA ALTERAR"
    changeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    changeButton.Font = Enum.Font.GothamBold
    changeButton.TextSize = 12
    
    local listening = false
    
    -- Mapeamento de teclas para nomes leg√≠veis
    local keyNames = {
        [Enum.KeyCode.F1] = "F1",
        [Enum.KeyCode.F2] = "F2",
        [Enum.KeyCode.F3] = "F3",
        [Enum.KeyCode.F4] = "F4",
        [Enum.KeyCode.F5] = "F5",
        [Enum.KeyCode.F6] = "F6",
        [Enum.KeyCode.F7] = "F7",
        [Enum.KeyCode.F8] = "F8",
        [Enum.KeyCode.F9] = "F9",
        [Enum.KeyCode.F10] = "F10",
        [Enum.KeyCode.F11] = "F11",
        [Enum.KeyCode.F12] = "F12",
        [Enum.KeyCode.Insert] = "Insert",
        [Enum.KeyCode.Delete] = "Delete",
        [Enum.KeyCode.Home] = "Home",
        [Enum.KeyCode.End] = "End",
        [Enum.KeyCode.PageUp] = "PageUp",
        [Enum.KeyCode.PageDown] = "PageDown",
        [Enum.KeyCode.RightShift] = "RightShift",
        [Enum.KeyCode.RightControl] = "RightControl",
        [Enum.KeyCode.LeftShift] = "LeftShift",
        [Enum.KeyCode.LeftControl] = "LeftControl",
        [Enum.KeyCode.CapsLock] = "CapsLock",
        [Enum.KeyCode.Tab] = "Tab"
    }
    
    -- Fun√ß√£o para obter nome leg√≠vel da tecla
    local function getKeyName(keyCode)
        return keyNames[keyCode] or tostring(keyCode):gsub("Enum.KeyCode.", "")
    end
    
    changeButton.MouseButton1Click:Connect(function()
        if not listening then
            listening = true
            changeButton.Text = "PRESSIONE UMA TECLA..."
            changeButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            
            local connection
            connection = UIS.InputBegan:Connect(function(input)
                if input.KeyCode ~= Enum.KeyCode.Unknown and input.KeyCode ~= Enum.KeyCode.Backspace then
                    -- Atualizar hotkey
                    toggleMenuKey = input.KeyCode
                    currentKeyText = getKeyName(input.KeyCode)
                    currentKeyLabel.Text = "Tecla atual: " .. currentKeyText
                    menuSettings.Text = "üéÆ CONFIGURA√á√ïES DO MENU\n\n‚Ä¢ Tecla atual: " .. currentKeyText .. "\n‚Ä¢ Arrastar menu: Segurar barra de t√≠tulo\n‚Ä¢ Fechar menu: Bot√£o X"
                    
                    -- Recriar conex√£o com nova hotkey
                    UIS.InputBegan:Connect(function(newInput)
                        if newInput.KeyCode == toggleMenuKey then
                            MainFrame.Visible = not MainFrame.Visible
                        end
                    end)
                    
                    -- Resetar bot√£o
                    listening = false
                    changeButton.Text = "CLIQUE PARA ALTERAR"
                    changeButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
                    
                    connection:Disconnect()
                end
            end)
        end
    end)
    
