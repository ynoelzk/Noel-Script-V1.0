    notaPortugues.Size = UDim2.new(0.9, 0, 0, 80)
    notaPortugues.Position = UDim2.new(0.05, 0, 0, 210)
    notaPortugues.BackgroundColor3 = Color3.fromRGB(30, 10, 10)
    notaPortugues.Text = "üí¨ Ol√°, sou yNoelz e criei esse script para voc√™!\n\nUse com respeito e modera√ß√£o!"
    notaPortugues.TextColor3 = Color3.fromRGB(255, 200, 100)
    notaPortugues.Font = Enum.Font.GothamBold
    notaPortugues.TextSize = 12
    notaPortugues.TextWrapped = true
    notaPortugues.TextXAlignment = Enum.TextXAlignment.Center
    
    -- Nota do desenvolvedor em ingl√™s
    local notaIngles = Instance.new("TextLabel", CreditosContent)
    notaIngles.Size = UDim2.new(0.9, 0, 0, 80)
    notaIngles.Position = UDim2.new(0.05, 0, 0, 300)
    notaIngles.BackgroundColor3 = Color3.fromRGB(10, 10, 30)
    notaIngles.Text = "üí¨ Hello, I'm yNoelz and I created this script for you!\n\nUse with respect and moderation!"
    notaIngles.TextColor3 = Color3.fromRGB(100, 200, 255)
    notaIngles.Font = Enum.Font.GothamBold
    notaIngles.TextSize = 12
    notaIngles.TextWrapped = true
    notaIngles.TextXAlignment = Enum.TextXAlignment.Center
    
    -- ============================================================================
    -- 11. BOT√ïES DAS ABAS
    -- ============================================================================
    
    local tabs = {}
    local tabContents = {}
    
    tabs.TCS = Instance.new("TextButton", TabPanel)
    tabs.TCS.Size = UDim2.new(1, -10, 0, 35)
    tabs.TCS.Position = UDim2.new(0, 5, 0, 20)
    tabs.TCS.BackgroundColor3 = Color3.fromRGB(128, 0, 32)
    tabs.TCS.Text = "‚öΩ TCS"
    tabs.TCS.TextColor3 = Color3.fromRGB(255, 255, 255)
    tabs.TCS.Font = Enum.Font.GothamBold
    tabs.TCS.TextSize = 14
    
    local otherTabs = {
        {"FPS", "üéØ", 70},
        {"NATURALDISASTER", "üå™Ô∏è", 120},
        {"ZARALHAR", "üåÄ", 170},
        {"CONFIGS", "‚öôÔ∏è", 220},
        {"CREDITOS", "üìú", 270}
    }
    
    for _, tab in ipairs(otherTabs) do
        tabs[tab[1]] = Instance.new("TextButton", TabPanel)
        tabs[tab[1]].Size = UDim2.new(1, -10, 0, 35)
        tabs[tab[1]].Position = UDim2.new(0, 5, 0, tab[3])
        tabs[tab[1]].BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        tabs[tab[1]].Text = tab[2] .. " " .. (tab[1] == "NATURALDISASTER" and "ND" or tab[1])
        tabs[tab[1]].TextColor3 = Color3.fromRGB(255, 255, 255)
        tabs[tab[1]].Font = Enum.Font.GothamBold
        tabs[tab[1]].TextSize = 14
    end
    
    -- ============================================================================
    -- 12. SISTEMA PARA TROCAR DE ABA
    -- ============================================================================
    
    -- Armazenar conte√∫dos das abas
    tabContents.TCS = TCSContent
    tabContents.FPS = FPSContent
    tabContents.NATURALDISASTER = NDContent
    tabContents.ZARALHAR = ZaralharContent
    tabContents.CONFIGS = ConfigContent
    tabContents.CREDITOS = CreditosContent
    
    local function switchTab(tabName)
        for _, content in pairs(tabContents) do
            content.Visible = false
        end
        
        for name, btn in pairs(tabs) do
            btn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        end
        
        if tabContents[tabName] then
            tabContents[tabName].Visible = true
            tabs[tabName].BackgroundColor3 = Color3.fromRGB(128, 0, 32)
        end
    end
    
    for name, btn in pairs(tabs) do
        btn.MouseButton1Click:Connect(function()
            switchTab(name)
        end)
    end
    
    -- ============================================================================
    -- 13. FUNCIONALIDADES EXTRAS
    -- ============================================================================
    
    -- Sistema de hotkey para mostrar/esconder menu
    UIS.InputBegan:Connect(function(input)
        if input.KeyCode == toggleMenuKey then
            MainFrame.Visible = not MainFrame.Visible
        end
    end)
    
    local dragging = false
    local dragStart, startPos
    
    TitleFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
        end
    end)
    
    TitleFrame.InputEnded:Connect(function()
        dragging = false
    end)
    
    UIS.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            MainFrame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
        end
    end)
    
