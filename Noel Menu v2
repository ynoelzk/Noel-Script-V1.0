    Container.Position = UDim2.new(0, 0, 0, 45)
    Container.BackgroundTransparency = 1
    
    -- Abas
    local TabPanel = Instance.new("Frame", Container)
    TabPanel.Size = UDim2.new(0, 120, 1, -10)
    TabPanel.Position = UDim2.new(0, 0, 0, 5)
    TabPanel.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
    
    -- Borda
    local Border = Instance.new("Frame", TabPanel)
