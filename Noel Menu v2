    local hotkeyTitle = Instance.new("TextLabel", hotkeyFrame)
    hotkeyTitle.Size = UDim2.new(1, 0, 0.3, 0)
    hotkeyTitle.Position = UDim2.new(0, 0, 0, 0)
    hotkeyTitle.BackgroundTransparency = 1
