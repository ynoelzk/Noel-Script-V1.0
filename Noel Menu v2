local aimbotSettings = {
    active = false,
    range = 500, -- Alcance em studs
    smoothness = 0.3, -- Suavidade do aim (0.1 = muito suave, 1 = instant√¢neo)
    targetPart = "Head", -- Parte do corpo para mirar
    requireTeamCheck = true -- S√≥ mirar em inimigos
}

-- Vari√°veis para ESP
local espSettings = {
    boxEnabled = false,
    circleEnabled = false,
    showTeam = false,
    maxDistance = 500
}

local espConnections = {}
local espObjects = {}

