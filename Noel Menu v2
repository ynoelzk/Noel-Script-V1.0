    local FPSContent = Instance.new("ScrollingFrame", ContentArea)
    FPSContent.Size = UDim2.new(1, 0, 1, 0)
    FPSContent.BackgroundTransparency = 1
    FPSContent.ScrollBarThickness = 6
    FPSContent.CanvasSize = UDim2.new(0, 0, 0, 700)
    FPSContent.Visible = false
    
    local FPSTitle = Instance.new("TextLabel", FPSContent)
    FPSTitle.Size = UDim2.new(1, -20, 0, 30)
    FPSTitle.Position = UDim2.new(0, 10, 0, 10)
    FPSTitle.BackgroundTransparency = 1
    FPSTitle.Text = "üéØ FPS CHEATS"
    FPSTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    FPSTitle.Font = Enum.Font.GothamBlack
    FPSTitle.TextSize = 16
    
    -- Fun√ß√£o para criar bot√£o de toggle
    local function createFPSToggleButton(text, yPos, initialState, description)
        local buttonFrame = Instance.new("Frame", FPSContent)
        buttonFrame.Size = UDim2.new(0.9, 0, 0, 60)
        buttonFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        buttonFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        buttonFrame.BorderSizePixel = 0
        
        local button = Instance.new("TextButton", buttonFrame)
        button.Size = UDim2.new(0.4, 0, 0.6, 0)
        button.Position = UDim2.new(0, 0, 0, 0)
        button.BackgroundColor3 = initialState and Color3.fromRGB(150, 60, 60) or Color3.fromRGB(60, 60, 150)
        button.Text = initialState and "‚ùå " .. text or "‚úÖ " .. text
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.GothamBold
        button.TextSize = 14
        
        local statusLabel = Instance.new("TextLabel", buttonFrame)
        statusLabel.Size = UDim2.new(0.4, 0, 0.6, 0)
        statusLabel.Position = UDim2.new(0.6, 0, 0, 0)
        statusLabel.BackgroundTransparency = 1
        statusLabel.Text = initialState and "ATIVO" or "INATIVO"
        statusLabel.TextColor3 = initialState and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(100, 255, 100)
        statusLabel.Font = Enum.Font.GothamBold
        statusLabel.TextSize = 12
        
        local descLabel = Instance.new("TextLabel", buttonFrame)
        descLabel.Size = UDim2.new(1, 0, 0.4, 0)
        descLabel.Position = UDim2.new(0, 0, 0.6, 0)
        descLabel.BackgroundTransparency = 1
        descLabel.Text = description
        descLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextSize = 11
        descLabel.TextWrapped = true
        descLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        return button, buttonFrame, statusLabel
    end
    
    -- Fun√ß√£o para criar slider de controle CORRIGIDA
    local function createSliderControl(yPos, title, minValue, maxValue, step, defaultValue, unit, onChange)
        local controlFrame = Instance.new("Frame", FPSContent)
        controlFrame.Size = UDim2.new(0.9, 0, 0, 60)
        controlFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        controlFrame.BackgroundTransparency = 1
        
        local titleLabel = Instance.new("TextLabel", controlFrame)
        titleLabel.Size = UDim2.new(1, 0, 0.4, 0)
        titleLabel.Position = UDim2.new(0, 0, 0, 0)
        titleLabel.BackgroundTransparency = 1
        titleLabel.Text = title .. ": " .. defaultValue .. unit
        titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        titleLabel.Font = Enum.Font.GothamBold
        titleLabel.TextSize = 13
        titleLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        local sliderFrame = Instance.new("Frame", controlFrame)
        sliderFrame.Size = UDim2.new(1, 0, 0.4, 0)
        sliderFrame.Position = UDim2.new(0, 0, 0.4, 0)
        sliderFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        sliderFrame.BorderSizePixel = 0
        
        local fillFrame = Instance.new("Frame", sliderFrame)
        fillFrame.Size = UDim2.new((defaultValue - minValue) / (maxValue - minValue), 0, 1, 0)
        fillFrame.Position = UDim2.new(0, 0, 0, 0)
        fillFrame.BackgroundColor3 = Color3.fromRGB(128, 0, 32)
        fillFrame.BorderSizePixel = 0
        
        local valueLabel = Instance.new("TextLabel", sliderFrame)
        valueLabel.Size = UDim2.new(1, 0, 1, 0)
        valueLabel.Position = UDim2.new(0, 0, 0, 0)
        valueLabel.BackgroundTransparency = 1
        valueLabel.Text = defaultValue .. unit
        valueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        valueLabel.Font = Enum.Font.GothamBold
        valueLabel.TextSize = 12
        valueLabel.TextXAlignment = Enum.TextXAlignment.Center
        
        local stepLabel = Instance.new("TextLabel", controlFrame)
        stepLabel.Size = UDim2.new(1, 0, 0.2, 0)
        stepLabel.Position = UDim2.new(0, 0, 0.8, 0)
        stepLabel.BackgroundTransparency = 1
        stepLabel.Text = "Min: " .. minValue .. unit .. " | Max: " .. maxValue .. unit .. " | Step: " .. step .. unit
        stepLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
        stepLabel.Font = Enum.Font.Gotham
        stepLabel.TextSize = 10
        
        local currentValue = defaultValue
        
        local function updateSlider()
            local percentage = (currentValue - minValue) / (maxValue - minValue)
            percentage = math.clamp(percentage, 0, 1)
            fillFrame.Size = UDim2.new(percentage, 0, 1, 0)
            titleLabel.Text = title .. ": " .. currentValue .. unit
            valueLabel.Text = currentValue .. unit
            if onChange then onChange(currentValue) end
        end
        
        -- Fun√ß√£o para clicar e arrastar CORRIGIDA
        local function onSliderInput(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                local connection
                local startMousePos = input.Position.X
                local startPercentage = (currentValue - minValue) / (maxValue - minValue)
                
                connection = RunService.Heartbeat:Connect(function()
                    local mousePos = UIS:GetMouseLocation().X
                    local sliderAbsPos = sliderFrame.AbsolutePosition.X
                    local sliderAbsSize = sliderFrame.AbsoluteSize.X
                    
                    local relativeX = (mousePos - sliderAbsPos) / sliderAbsSize
                    relativeX = math.clamp(relativeX, 0, 1)
                    
                    -- Calcular novo valor com step
                    local rawValue = minValue + (maxValue - minValue) * relativeX
                    currentValue = math.floor((rawValue + step/2) / step) * step
                    currentValue = math.clamp(currentValue, minValue, maxValue)
                    
                    updateSlider()
                end)
                
                local function disconnect()
                    if connection then
                        connection:Disconnect()
                        connection = nil
                    end
                end
                
                local endedConnection
                endedConnection = UIS.InputEnded:Connect(function(endInput)
                    if endInput.UserInputType == Enum.UserInputType.MouseButton1 then
                        disconnect()
                        if endedConnection then
                            endedConnection:Disconnect()
                        end
                    end
                end)
            end
        end
        
        sliderFrame.InputBegan:Connect(onSliderInput)
        updateSlider()
        
        return controlFrame, currentValue
    end
    
    -- 1. AIMBOT TOGGLE
    local aimbotButton, aimbotFrame, aimbotStatus = createFPSToggleButton("AIMBOT", 50, false, "Mira autom√°tica avan√ßada (Bot√£o direito do mouse)")
    
    -- 2. CONTROLE DE ALCANCE (CORRIGIDO)
    local rangeControl, rangeValue = createSliderControl(120, "Alcance do Aimbot", 50, 1000, 10, aimbotSettings.range, " studs", function(value)
        aimbotSettings.range = value
    end)
    
    -- 3. CONTROLE DE SUAVIDADE (CORRIGIDO)
    local smoothControl, smoothValue = createSliderControl(190, "Suavidade", 0.1, 1, 0.05, aimbotSettings.smoothness, "", function(value)
        aimbotSettings.smoothness = value
    end)
    
    -- 4. ESP BOX
    local espBoxButton, espBoxFrame, espBoxStatus = createFPSToggleButton("ESP BOX", 260, false, "Mostra caixa ao redor dos jogadores")
    
    -- 5. ESP CIRCLE
    local espCircleButton, espCircleFrame, espCircleStatus = createFPSToggleButton("ESP CIRCLE", 330, false, "Mostra c√≠rculo nos p√©s dos jogadores")
    
    -- Sistema de aimbot complexo
    local aimbotConnection = nil
    local rightMouseDown = false
    
    -- Conex√£o do bot√£o direito do mouse
    local mouseButton2Connection = UIS.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton2 then
            rightMouseDown = true
        end
    end)
    
    local mouseButton2Released = UIS.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton2 then
            rightMouseDown = false
        end
    end)
    
    -- Fun√ß√£o para encontrar o jogador mais pr√≥ximo
    local function getClosestPlayer()
        local localPlayer = Players.LocalPlayer
        local localCharacter = localPlayer.Character
        if not localCharacter then return nil end
        
        local localHead = localCharacter:FindFirstChild("Head")
        if not localHead then return nil end
        
        local camera = Workspace.CurrentCamera
        if not camera then return nil end
        
        local closestPlayer = nil
        local closestDistance = math.huge
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= localPlayer then
                -- Verificar se √© inimigo (se team check estiver ativado)
                if aimbotSettings.requireTeamCheck then
                    if player.Team and localPlayer.Team and player.Team == localPlayer.Team then
                        continue
                    end
                end
                
                local character = player.Character
                if character and character:FindFirstChild("Humanoid") and character.Humanoid.Health > 0 then
                    local targetPart = character:FindFirstChild(aimbotSettings.targetPart)
                    if targetPart then
                        local distance = (localHead.Position - targetPart.Position).Magnitude
                        
                        -- Verificar se est√° dentro do alcance
                        if distance <= aimbotSettings.range then
                            if distance < closestDistance then
                                closestDistance = distance
                                closestPlayer = player
                            end
                        end
                    end
                end
            end
        end
        
        return closestPlayer
    end
    
    aimbotButton.MouseButton1Click:Connect(function()
        aimbotSettings.active = not aimbotSettings.active
        
        if aimbotSettings.active then
            aimbotButton.Text = "‚ùå AIMBOT"
            aimbotButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            aimbotStatus.Text = "ATIVO"
            aimbotStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            -- Conex√£o do aimbot
            aimbotConnection = RunService.Heartbeat:Connect(function()
                if aimbotSettings.active and rightMouseDown then
                    local closestPlayer = getClosestPlayer()
                    if closestPlayer and closestPlayer.Character then
                        local targetPart = closestPlayer.Character:FindFirstChild(aimbotSettings.targetPart)
                        if targetPart then
                            local camera = Workspace.CurrentCamera
                            if camera then
                                -- C√°lculo de mira suave
                                local currentCFrame = camera.CFrame
                                local targetPosition = targetPart.Position
                                
                                -- Adicionar predi√ß√£o de movimento
                                local targetVelocity = targetPart.Velocity or Vector3.new(0, 0, 0)
                                local distance = (camera.CFrame.Position - targetPosition).Magnitude
                                local travelTime = distance / 1000
                                targetPosition = targetPosition + (targetVelocity * travelTime)
                                
                                -- Interpola√ß√£o suave
                                local newLookVector = (targetPosition - currentCFrame.Position).Unit
                                local currentLookVector = currentCFrame.LookVector
                                
                                -- Aplicar suavidade ajustada
                                local smoothedLookVector = currentLookVector:Lerp(newLookVector, aimbotSettings.smoothness)
                                
                                -- Aplicar nova rota√ß√£o da c√¢mera
                                camera.CFrame = CFrame.new(currentCFrame.Position, currentCFrame.Position + smoothedLookVector)
                            end
                        end
                    end
                end
            end)
        else
            aimbotButton.Text = "‚úÖ AIMBOT"
            aimbotButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            aimbotStatus.Text = "INATIVO"
            aimbotStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if aimbotConnection then
                aimbotConnection:Disconnect()
                aimbotConnection = nil
            end
        end
    end)
    
    -- Sistema de ESP
    local function createESPBox(player)
        if not player.Character then return end
        
        local box = Instance.new("BoxHandleAdornment")
        box.Name = player.Name .. "_ESPBOX"
        box.Adornee = player.Character
        box.AlwaysOnTop = true
        box.ZIndex = 10
        box.Size = Vector3.new(4, 6, 1)
        box.Color3 = player.Team and player.Team == Players.LocalPlayer.Team and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
        box.Transparency = 0.7
        box.Parent = player.Character
        
        espObjects[player.Name .. "_BOX"] = box
        
        return box
    end
    
    local function createESPCircle(player)
        if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then return end
        
        local circle = Instance.new("BillboardGui")
        circle.Name = player.Name .. "_ESPCIRCLE"
        circle.Adornee = player.Character.HumanoidRootPart
        circle.AlwaysOnTop = true
        circle.Size = UDim2.new(2, 0, 2, 0)
        circle.StudsOffset = Vector3.new(0, -3, 0)
        
        local frame = Instance.new("Frame", circle)
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundTransparency = 1
        
        local uiCorner = Instance.new("UICorner", frame)
        uiCorner.CornerRadius = UDim.new(1, 0)
        
        local uiStroke = Instance.new("UIStroke", frame)
        uiStroke.Color = player.Team and player.Team == Players.LocalPlayer.Team and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
        uiStroke.Thickness = 3
        uiStroke.Transparency = 0.5
        
        circle.Parent = player.Character.HumanoidRootPart
        
        espObjects[player.Name .. "_CIRCLE"] = circle
        
        return circle
    end
    
    local function updateESP()
        -- Limpar ESP antigo
        for _, obj in pairs(espObjects) do
            if obj then
                obj:Destroy()
            end
        end
        espObjects = {}
        
        -- Criar novo ESP
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= Players.LocalPlayer and player.Character then
                if espSettings.boxEnabled then
                    createESPBox(player)
                end
                if espSettings.circleEnabled then
                    createESPCircle(player)
                end
            end
        end
    end
    
    local function onPlayerAdded(player)
        player.CharacterAdded:Connect(function()
            if espSettings.boxEnabled or espSettings.circleEnabled then
                wait(0.5)
                updateESP()
            end
        end)
    end
    
    -- Conectar eventos de jogadores
    for _, player in ipairs(Players:GetPlayers()) do
        onPlayerAdded(player)
    end
    Players.PlayerAdded:Connect(onPlayerAdded)
    
    -- ESP Box Toggle
    espBoxButton.MouseButton1Click:Connect(function()
        espSettings.boxEnabled = not espSettings.boxEnabled
        
        if espSettings.boxEnabled then
            espBoxButton.Text = "‚ùå ESP BOX"
            espBoxButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            espBoxStatus.Text = "ATIVO"
            espBoxStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        else
            espBoxButton.Text = "‚úÖ ESP BOX"
            espBoxButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            espBoxStatus.Text = "INATIVO"
            espBoxStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        updateESP()
    end)
    
    -- ESP Circle Toggle
    espCircleButton.MouseButton1Click:Connect(function()
        espSettings.circleEnabled = not espSettings.circleEnabled
        
        if espSettings.circleEnabled then
            espCircleButton.Text = "‚ùå ESP CIRCLE"
            espCircleButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            espCircleStatus.Text = "ATIVO"
            espCircleStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        else
            espCircleButton.Text = "‚úÖ ESP CIRCLE"
            espCircleButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            espCircleStatus.Text = "INATIVO"
            espCircleStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        updateESP()
    end)
    
    -- Atualizar ESP quando personagem morre
    Players.LocalPlayer.CharacterAdded:Connect(function()
        wait(1)
        updateESP()
    end)
    
