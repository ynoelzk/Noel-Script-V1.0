    keysInfo.Size = UDim2.new(0.9, 0, 0, 80)
    keysInfo.Position = UDim2.new(0.05, 0, 0, 270)
    keysInfo.BackgroundColor3 = Color3.fromRGB(25, 25, 50)
    keysInfo.Text = "üí° TECLAS RECOMENDADAS\n\n‚Ä¢ F1 a F12: Teclas de fun√ß√£o\n‚Ä¢ Insert, Delete, Home, End\n‚Ä¢ PageUp, PageDown\n‚Ä¢ RightShift, RightControl"
    keysInfo.TextColor3 = Color3.fromRGB(200, 255, 200)
    keysInfo.Font = Enum.Font.Gotham
    keysInfo.TextSize = 11
    keysInfo.TextWrapped = true
    
    -- Informa√ß√µes do sistema
    local systemInfo = Instance.new("TextLabel", ConfigContent)
    systemInfo.Size = UDim2.new(0.9, 0, 0, 80)
    systemInfo.Position = UDim2.new(0.05, 0, 0, 360)
    systemInfo.BackgroundColor3 = Color3.fromRGB(50, 25, 25)
    systemInfo.Text = "üìä INFORMA√á√ïES DO SISTEMA\n\n‚Ä¢ Menu: NOEL MENU V2.0\n‚Ä¢ Desenvolvedor: yNoelz\n‚Ä¢ Status: INDETECTAVEL"
    systemInfo.TextColor3 = Color3.fromRGB(255, 200, 100)
    systemInfo.Font = Enum.Font.GothamBold
    systemInfo.TextSize = 12
    systemInfo.TextWrapped = true
    
    -- ============================================================================
    -- 10. ABA CR√âDITOS
    -- ============================================================================
    
    local CreditosContent = Instance.new("ScrollingFrame", ContentArea)
    CreditosContent.Size = UDim2.new(1, 0, 1, 0)
    CreditosContent.BackgroundTransparency = 1
    CreditosContent.ScrollBarThickness = 6
    CreditosContent.CanvasSize = UDim2.new(0, 0, 0, 400)
    CreditosContent.Visible = false
    
    local CreditosTitle = Instance.new("TextLabel", CreditosContent)
    CreditosTitle.Size = UDim2.new(1, -20, 0, 40)
    CreditosTitle.Position = UDim2.new(0, 10, 0, 10)
    CreditosTitle.BackgroundTransparency = 1
    CreditosTitle.Text = "üìú CR√âDITOS"
    CreditosTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    CreditosTitle.Font = Enum.Font.GothamBlack
    CreditosTitle.TextSize = 20
    CreditosTitle.TextXAlignment = Enum.TextXAlignment.Center
    
    -- Adicionar contorno marsala ao t√≠tulo dos cr√©ditos
    local CreditosTitleStroke = Instance.new("UIStroke", CreditosTitle)
    CreditosTitleStroke.Color = Color3.fromRGB(128, 0, 32)
    CreditosTitleStroke.Thickness = 2
    CreditosTitleStroke.Transparency = 0
    
    -- Informa√ß√µes dos cr√©ditos
    local creditsInfo = {
        {"üëë DESENVOLVEDOR", "yNoelz", 70},
        {"üìÖ VERS√ÉO", "V2.0", 110},
        {"üîí SEGURAN√áA", "INDETECTAVEL", 150}
    }
    
    for _, info in ipairs(creditsInfo) do
        local creditFrame = Instance.new("Frame", CreditosContent)
        creditFrame.Size = UDim2.new(0.9, 0, 0, 40)
        creditFrame.Position = UDim2.new(0.05, 0, 0, info[3])
        creditFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        
        local titleLabel = Instance.new("TextLabel", creditFrame)
        titleLabel.Size = UDim2.new(0.5, 0, 1, 0)
        titleLabel.Position = UDim2.new(0, 0, 0, 0)
        titleLabel.BackgroundTransparency = 1
        titleLabel.Text = info[1]
        titleLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
        titleLabel.Font = Enum.Font.Gotham
        titleLabel.TextSize = 12
        titleLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        local valueLabel = Instance.new("TextLabel", creditFrame)
        valueLabel.Size = UDim2.new(0.5, 0, 1, 0)
        valueLabel.Position = UDim2.new(0.5, 0, 0, 0)
        valueLabel.BackgroundTransparency = 1
        valueLabel.Text = info[2]
        if info[1] == "üîí SEGURAN√áA" then
            valueLabel.TextColor3 = Color3.fromRGB(100, 255, 100) -- Verde para INDETECTAVEL
        else
            valueLabel.TextColor3 = Color3.fromRGB(128, 0, 32) -- Marsala para outros
        end
        valueLabel.Font = Enum.Font.GothamBold
        valueLabel.TextSize = 14
        valueLabel.TextXAlignment = Enum.TextXAlignment.Right
