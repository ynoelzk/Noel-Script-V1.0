    -- ============================================================================
    -- 8. ABA ZARALHAR
    -- ============================================================================
    
    local ZaralharContent = Instance.new("ScrollingFrame", ContentArea)
    ZaralharContent.Size = UDim2.new(1, 0, 1, 0)
    ZaralharContent.BackgroundTransparency = 1
    ZaralharContent.ScrollBarThickness = 6
    ZaralharContent.CanvasSize = UDim2.new(0, 0, 0, 570)
    ZaralharContent.Visible = false
    
    local ZaralharTitle = Instance.new("TextLabel", ZaralharContent)
    ZaralharTitle.Size = UDim2.new(1, -20, 0, 30)
    ZaralharTitle.Position = UDim2.new(0, 10, 0, 10)
    ZaralharTitle.BackgroundTransparency = 1
    ZaralharTitle.Text = "üåÄ ZARALHAR CHEATS"
    ZaralharTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    ZaralharTitle.Font = Enum.Font.GothamBlack
    ZaralharTitle.TextSize = 16
    
    -- Vari√°veis globais para os cheats
    local player = Players.LocalPlayer
    local flyActive = false
    local flyBodyVelocity = nil
    local flyConnection = nil
    local noclipActive = false
    local noclipConnection = nil
    local godModeActive = false
    local godModeConnection = nil
    
    -- Configura√ß√µes do fly
    local flySpeed = 50
    
    -- Fun√ß√£o para criar um bot√£o de toggle com status
    local function createToggleButton(text, yPos, initialState, description)
        local buttonFrame = Instance.new("Frame", ZaralharContent)
        buttonFrame.Size = UDim2.new(0.9, 0, 0, 60)
        buttonFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        buttonFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        buttonFrame.BorderSizePixel = 0
        
        local button = Instance.new("TextButton", buttonFrame)
        button.Size = UDim2.new(0.4, 0, 0.6, 0)
        button.Position = UDim2.new(0, 0, 0, 0)
        button.BackgroundColor3 = initialState and Color3.fromRGB(150, 60, 60) or Color3.fromRGB(60, 60, 150)
        button.Text = initialState and "‚ùå " .. text or "‚úÖ " .. text
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.GothamBold
        button.TextSize = 14
        
        local statusLabel = Instance.new("TextLabel", buttonFrame)
        statusLabel.Size = UDim2.new(0.4, 0, 0.6, 0)
        statusLabel.Position = UDim2.new(0.6, 0, 0, 0)
        statusLabel.BackgroundTransparency = 1
        statusLabel.Text = initialState and "ATIVO" or "INATIVO"
        statusLabel.TextColor3 = initialState and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(100, 255, 100)
        statusLabel.Font = Enum.Font.GothamBold
        statusLabel.TextSize = 12
        
        local descLabel = Instance.new("TextLabel", buttonFrame)
        descLabel.Size = UDim2.new(1, 0, 0.4, 0)
        descLabel.Position = UDim2.new(0, 0, 0.6, 0)
        descLabel.BackgroundTransparency = 1
        descLabel.Text = description
        descLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextSize = 11
        descLabel.TextWrapped = true
        descLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        return button, buttonFrame, statusLabel
    end
    
    -- Fun√ß√£o para criar controles de velocidade do fly
    local function createFlySpeedControl(yPos)
        local controlFrame = Instance.new("Frame", ZaralharContent)
        controlFrame.Size = UDim2.new(0.9, 0, 0, 40)
        controlFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        controlFrame.BackgroundTransparency = 1
        
        local label = Instance.new("TextLabel", controlFrame)
        label.Size = UDim2.new(0.5, 0, 1, 0)
        label.Position = UDim2.new(0, 0, 0, 0)
        label.BackgroundTransparency = 1
        label.Text = "Fly Speed: " .. flySpeed
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.Font = Enum.Font.GothamBold
        label.TextSize = 13
        label.TextXAlignment = Enum.TextXAlignment.Left
        
        local buttonFrame = Instance.new("Frame", controlFrame)
        buttonFrame.Size = UDim2.new(0.5, 0, 1, 0)
        buttonFrame.Position = UDim2.new(0.5, 0, 0, 0)
        buttonFrame.BackgroundTransparency = 1
        
        local minusButton = Instance.new("TextButton", buttonFrame)
        minusButton.Size = UDim2.new(0.2, 0, 1, 0)
        minusButton.Position = UDim2.new(0, 0, 0, 0)
        minusButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        minusButton.Text = "-"
        minusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        minusButton.Font = Enum.Font.GothamBold
        minusButton.TextSize = 14
        
        local valueLabel = Instance.new("TextLabel", buttonFrame)
        valueLabel.Size = UDim2.new(0.6, 0, 1, 0)
        valueLabel.Position = UDim2.new(0.2, 0, 0, 0)
        valueLabel.BackgroundTransparency = 1
        valueLabel.Text = tostring(flySpeed)
        valueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        valueLabel.Font = Enum.Font.GothamBold
        valueLabel.TextSize = 14
        
        local plusButton = Instance.new("TextButton", buttonFrame)
        plusButton.Size = UDim2.new(0.2, 0, 1, 0)
        plusButton.Position = UDim2.new(0.8, 0, 0, 0)
        plusButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        plusButton.Text = "+"
        plusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        plusButton.Font = Enum.Font.GothamBold
        plusButton.TextSize = 14
        
        minusButton.MouseButton1Click:Connect(function()
            flySpeed = math.max(10, flySpeed - 10)
            valueLabel.Text = tostring(flySpeed)
            label.Text = "Fly Speed: " .. flySpeed
        end)
        
        plusButton.MouseButton1Click:Connect(function()
            flySpeed = math.min(200, flySpeed + 10)
            valueLabel.Text = tostring(flySpeed)
            label.Text = "Fly Speed: " .. flySpeed
        end)
        
        return controlFrame
    end
    
    -- Fun√ß√£o para criar controles de 5 em 5
    local function createIncrementalControl(yPos, title, currentValue, minValue, maxValue, step, onChange)
        local controlFrame = Instance.new("Frame", ZaralharContent)
        controlFrame.Size = UDim2.new(0.9, 0, 0, 40)
        controlFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        controlFrame.BackgroundTransparency = 1
        
        local label = Instance.new("TextLabel", controlFrame)
        label.Size = UDim2.new(0.5, 0, 1, 0)
        label.Position = UDim2.new(0, 0, 0, 0)
        label.BackgroundTransparency = 1
        label.Text = title .. ": " .. currentValue
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.Font = Enum.Font.GothamBold
        label.TextSize = 13
        label.TextXAlignment = Enum.TextXAlignment.Left
        
        local buttonFrame = Instance.new("Frame", controlFrame)
        buttonFrame.Size = UDim2.new(0.5, 0, 1, 0)
        buttonFrame.Position = UDim2.new(0.5, 0, 0, 0)
        buttonFrame.BackgroundTransparency = 1
        
        local minusButton = Instance.new("TextButton", buttonFrame)
        minusButton.Size = UDim2.new(0.2, 0, 1, 0)
        minusButton.Position = UDim2.new(0, 0, 0, 0)
        minusButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        minusButton.Text = "-" .. step
        minusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        minusButton.Font = Enum.Font.GothamBold
        minusButton.TextSize = 12
        
        local valueLabel = Instance.new("TextLabel", buttonFrame)
        valueLabel.Size = UDim2.new(0.6, 0, 1, 0)
        valueLabel.Position = UDim2.new(0.2, 0, 0, 0)
        valueLabel.BackgroundTransparency = 1
        valueLabel.Text = tostring(currentValue)
        valueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        valueLabel.Font = Enum.Font.GothamBold
        valueLabel.TextSize = 14
        
        local plusButton = Instance.new("TextButton", buttonFrame)
        plusButton.Size = UDim2.new(0.2, 0, 1, 0)
        plusButton.Position = UDim2.new(0.8, 0, 0, 0)
        plusButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        plusButton.Text = "+" .. step
        plusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        plusButton.Font = Enum.Font.GothamBold
        plusButton.TextSize = 12
        
        minusButton.MouseButton1Click:Connect(function()
            currentValue = math.max(minValue, currentValue - step)
            valueLabel.Text = tostring(currentValue)
            label.Text = title .. ": " .. currentValue
            if onChange then onChange(currentValue) end
        end)
        
        plusButton.MouseButton1Click:Connect(function()
            currentValue = math.min(maxValue, currentValue + step)
            valueLabel.Text = tostring(currentValue)
            label.Text = title .. ": " .. currentValue
            if onChange then onChange(currentValue) end
        end)
        
        return controlFrame, currentValue
    end
    
    -- 1. FLY COM CONTROLE DE VELOCIDADE
    local flyButton, flyFrame, flyStatus = createToggleButton("FLY", 50, false, "Permite voar pelo mapa (E=Subir, Q=Descer, WASD=Dire√ß√£o)")
    
    local flySpeedControl = createFlySpeedControl(120)
    
    flyButton.MouseButton1Click:Connect(function()
        flyActive = not flyActive
        
        if flyActive then
            flyButton.Text = "‚ùå FLY"
            flyButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            flyStatus.Text = "ATIVO"
            flyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            humanoid.PlatformStand = true
            
            flyBodyVelocity = Instance.new("BodyVelocity")
            flyBodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
            flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
            flyBodyVelocity.Parent = character.PrimaryPart
            
            flyConnection = RunService.Heartbeat:Connect(function()
                if character and character.PrimaryPart and flyActive then
                    local camera = workspace.CurrentCamera
                    local moveDirection = Vector3.new(0, 0, 0)
                    
                    if UIS:IsKeyDown(Enum.KeyCode.W) then
                        moveDirection = moveDirection + camera.CFrame.LookVector
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.S) then
                        moveDirection = moveDirection - camera.CFrame.LookVector
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.D) then
                        moveDirection = moveDirection + camera.CFrame.RightVector
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.A) then
                        moveDirection = moveDirection - camera.CFrame.RightVector
                    end
                    
                    if UIS:IsKeyDown(Enum.KeyCode.E) then
                        moveDirection = moveDirection + Vector3.new(0, 1, 0)
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.Q) then
                        moveDirection = moveDirection + Vector3.new(0, -1, 0)
                    end
                    
                    if moveDirection.Magnitude > 0 then
                        moveDirection = moveDirection.Unit * flySpeed
                    end
                    
                    flyBodyVelocity.Velocity = moveDirection
                end
            end)
        else
            flyButton.Text = "‚úÖ FLY"
            flyButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            flyStatus.Text = "INATIVO"
            flyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if flyConnection then
                flyConnection:Disconnect()
                flyConnection = nil
            end
            
            local character = player.Character
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.PlatformStand = false
                end
                
                if flyBodyVelocity then
                    flyBodyVelocity:Destroy()
                    flyBodyVelocity = nil
                end
                
                character.PrimaryPart.Velocity = Vector3.new(0, 0, 0)
            end
        end
    end)
    
    -- 2. WALKSPEED
    local wsCurrentValue = 16
    local wsMinValue = 16
    local wsMaxValue = 150
    local wsStep = 5
    
    local wsButton, wsFrame, wsStatus = createToggleButton("WALKSPEED", 170, false, "Aumenta/diminui velocidade de movimento (de 5 em 5)")
    local wsControl, wsCurrentValue = createIncrementalControl(240, "WalkSpeed", wsCurrentValue, wsMinValue, wsMaxValue, wsStep, function(newValue)
        wsCurrentValue = newValue
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = wsCurrentValue
            end
        end
    end)
    
    wsButton.MouseButton1Click:Connect(function()
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        if wsCurrentValue > wsMinValue then
            wsCurrentValue = wsMinValue
            humanoid.WalkSpeed = wsCurrentValue
            wsButton.Text = "‚úÖ WALKSPEED"
            wsButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            wsStatus.Text = "INATIVO"
            wsStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        else
            humanoid.WalkSpeed = wsCurrentValue
            wsButton.Text = "‚ö° WS: " .. wsCurrentValue
            wsButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            wsStatus.Text = "ATIVO"
            wsStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    end)
    
    -- 3. JUMP POWER
    local jpCurrentValue = 50
    local jpMinValue = 50
    local jpMaxValue = 200
    local jpStep = 5
    
    local jpButton, jpFrame, jpStatus = createToggleButton("JUMP POWER", 290, false, "Aumenta/diminui o poder do pulo (de 5 em 5)")
    local jpControl, jpCurrentValue = createIncrementalControl(360, "Jump Power", jpCurrentValue, jpMinValue, jpMaxValue, jpStep, function(newValue)
        jpCurrentValue = newValue
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.JumpPower = jpCurrentValue
            end
        end
    end)
    
    jpButton.MouseButton1Click:Connect(function()
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        if jpCurrentValue > jpMinValue then
            jpCurrentValue = jpMinValue
            humanoid.JumpPower = jpCurrentValue
            jpButton.Text = "‚úÖ JUMP POWER"
            jpButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            jpStatus.Text = "INATIVO"
            jpStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        else
            humanoid.JumpPower = jpCurrentValue
            jpButton.Text = "ü¶ò JP: " .. jpCurrentValue
            jpButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            jpStatus.Text = "ATIVO"
            jpStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    end)
    
    -- 4. GOD MODE
    local godButton, godFrame, godStatus = createToggleButton("GOD MODE", 430, false, "Torna-se completamente invenc√≠vel")
    
    godButton.MouseButton1Click:Connect(function()
        godModeActive = not godModeActive
        
        if godModeActive then
            godButton.Text = "‚ùå GOD MODE"
            godButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            godStatus.Text = "ATIVO"
            godStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            
            humanoid:SetAttribute("OriginalMaxHealth", humanoid.MaxHealth)
            humanoid:SetAttribute("OriginalHealth", humanoid.Health)
            
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            
            godModeConnection = humanoid.HealthChanged:Connect(function()
                humanoid.Health = humanoid.MaxHealth
            end)
        else
            godButton.Text = "‚úÖ GOD MODE"
            godButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            godStatus.Text = "INATIVO"
            godStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if godModeConnection then
                godModeConnection:Disconnect()
                godModeConnection = nil
            end
            
            local character = player.Character
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    local originalMaxHealth = humanoid:GetAttribute("OriginalMaxHealth") or 100
                    local originalHealth = humanoid:GetAttribute("OriginalHealth") or 100
                    
                    humanoid.MaxHealth = originalMaxHealth
                    humanoid.Health = math.min(originalHealth, originalMaxHealth)
                end
            end
        end
    end)
    
    -- 5. NOCLIP
    local noclipButton, noclipFrame, noclipStatus = createToggleButton("NOCLIP", 500, false, "Atravessa paredes e objetos")
    
    noclipButton.MouseButton1Click:Connect(function()
        noclipActive = not noclipActive
        
        if noclipActive then
            noclipButton.Text = "‚ùå NOCLIP"
            noclipButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            noclipStatus.Text = "ATIVO"
            noclipStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            noclipConnection = RunService.Stepped:Connect(function()
                local character = player.Character
                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end
            end)
        else
            noclipButton.Text = "‚úÖ NOCLIP"
            noclipButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            noclipStatus.Text = "INATIVO"
            noclipStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if noclipConnection then
                noclipConnection:Disconnect()
                noclipConnection = nil
            end
            
            local character = player.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
        end
    end)
    
    -- Resetar cheats quando o personagem morre
    player.CharacterAdded:Connect(function()
        if flyActive then
            flyActive = false
            if flyConnection then flyConnection:Disconnect() end
            if flyBodyVelocity then flyBodyVelocity:Destroy() end
            flyButton.Text = "‚úÖ FLY"
            flyButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            flyStatus.Text = "INATIVO"
            flyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        if godModeActive then
            godModeActive = false
            if godModeConnection then godModeConnection:Disconnect() end
            godButton.Text = "‚úÖ GOD MODE"
            godButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            godStatus.Text = "INATIVO"
            godStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        if noclipActive then
            noclipActive = false
            if noclipConnection then noclipConnection:Disconnect() end
            noclipButton.Text = "‚úÖ NOCLIP"
            noclipButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            noclipStatus.Text = "INATIVO"
            noclipStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        task.wait(0.5)
        local character = player.Character
        if character then
            local humanoid = character:WaitForChild("Humanoid")
            
            if wsCurrentValue > wsMinValue then
                humanoid.WalkSpeed = wsCurrentValue
                wsButton.Text = "‚ö° WS: " .. wsCurrentValue
                wsButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
                wsStatus.Text = "ATIVO"
                wsStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            end
            
            if jpCurrentValue > jpMinValue then
                humanoid.JumpPower = jpCurrentValue
                jpButton.Text = "ü¶ò JP: " .. jpCurrentValue
                jpButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
                jpStatus.Text = "ATIVO"
                jpStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            end
        end
    end)
    
    -- ============================================================================
    -- 9. ABA CONFIGURA√á√ïES (COM HOTKEY SIMPLIFICADA)
    -- ============================================================================
    
    local ConfigContent = Instance.new("ScrollingFrame", ContentArea)
    ConfigContent.Size = UDim2.new(1, 0, 1, 0)
    ConfigContent.BackgroundTransparency = 1
    ConfigContent.ScrollBarThickness = 6
    ConfigContent.CanvasSize = UDim2.new(0, 0, 0, 400)
