-- üéÆ NOEL TOXIC SUITE - VERS√ÉO SEGURA (SEM CHEATS)
local CoreGui = game:GetService("CoreGui")
local UIS = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

-- ============================================================================
-- 1. ESTRUTURA B√ÅSICA
-- ============================================================================

local ScreenGui = Instance.new("ScreenGui", CoreGui)
ScreenGui.ResetOnSpawn = false

local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Size = UDim2.new(0, 320, 0, 380)
MainFrame.Position = UDim2.new(0.5, -160, 0.5, -190)
MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
MainFrame.BorderSizePixel = 2
MainFrame.BorderColor3 = Color3.fromRGB(128, 0, 32)
MainFrame.Visible = true

-- ============================================================================
-- 2. TELA DE LOGIN (SEGURA - SEM DICA DA SENHA)
-- ============================================================================

local Title = Instance.new("TextLabel", MainFrame)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundColor3 = Color3.fromRGB(5, 5, 5)
Title.Text = "NOEL TOXIC SUITE"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.GothamBlack
Title.TextSize = 18

local KeyBox = Instance.new("TextBox", MainFrame)
KeyBox.Size = UDim2.new(0.8, 0, 0, 30)
KeyBox.Position = UDim2.new(0.1, 0, 0.4, 0)
KeyBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
KeyBox.PlaceholderText = "Digite a senha de acesso"
KeyBox.Text = ""

local Button = Instance.new("TextButton", MainFrame)
Button.Size = UDim2.new(0.8, 0, 0, 30)
Button.Position = UDim2.new(0.1, 0, 0.6, 0)
Button.BackgroundColor3 = Color3.fromRGB(128, 0, 32)
Button.Text = "ENTRAR"
Button.TextColor3 = Color3.fromRGB(255, 255, 255)
Button.Font = Enum.Font.GothamBold

-- ============================================================================
-- 3. QUANDO CLICA NO BOT√ÉO
-- ============================================================================

Button.MouseButton1Click:Connect(function()
    if KeyBox.Text == "noel2026" then
        -- Remover elementos de login
        Title:Destroy()
        KeyBox:Destroy()
        Button:Destroy()
        
        -- Criar interface principal
        createMainInterface()
    else
        KeyBox.Text = ""
    end
end)

-- ============================================================================
-- 4. INTERFACE PRINCIPAL
-- ============================================================================

function createMainInterface()
    -- Ajustar frame
    MainFrame.Size = UDim2.new(0, 400, 0, 450)
    MainFrame.Position = UDim2.new(0.5, -200, 0.5, -225)
    MainFrame.BorderSizePixel = 3
    
    -- T√≠tulo
    local MainTitle = Instance.new("TextLabel", MainFrame)
    MainTitle.Size = UDim2.new(1, 0, 0, 45)
    MainTitle.BackgroundColor3 = Color3.fromRGB(8, 8, 8)
    MainTitle.Text = "NOEL TOXIC SUITE - VERS√ÉO SEGURA"
    MainTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    MainTitle.Font = Enum.Font.GothamBlack
    MainTitle.TextSize = 18
    
    -- BOT√ÉO X PARA FECHAR (NOVO)
    local CloseButton = Instance.new("TextButton", MainFrame)
    CloseButton.Size = UDim2.new(0, 25, 0, 25)
    CloseButton.Position = UDim2.new(1, -30, 0, 10)
    CloseButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    CloseButton.Text = "X"
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.TextSize = 14
    
    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
    
    -- Container
    local Container = Instance.new("Frame", MainFrame)
    Container.Size = UDim2.new(1, 0, 1, -45)
    Container.Position = UDim2.new(0, 0, 0, 45)
    Container.BackgroundTransparency = 1
    
    -- Abas
    local TabPanel = Instance.new("Frame", Container)
    TabPanel.Size = UDim2.new(0, 100, 1, -10)
    TabPanel.Position = UDim2.new(0, 0, 0, 5)
    TabPanel.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
    
    -- Borda
    local Border = Instance.new("Frame", TabPanel)
    Border.Size = UDim2.new(0, 2, 1, 0)
    Border.Position = UDim2.new(1, -2, 0, 0)
    Border.BackgroundColor3 = Color3.fromRGB(128, 0, 32)
    
    -- Conte√∫do
    local ContentArea = Instance.new("Frame", Container)
    ContentArea.Size = UDim2.new(1, -100, 1, -10)
    ContentArea.Position = UDim2.new(0, 100, 0, 5)
    ContentArea.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
    
    -- ============================================================================
    -- 5. ABA TCS - APENAS INFORMA√á√ïES (SEM CHEATS)
    -- ============================================================================
    
    local TCSContent = Instance.new("ScrollingFrame", ContentArea)
    TCSContent.Size = UDim2.new(1, 0, 1, 0)
    TCSContent.BackgroundTransparency = 1
    TCSContent.ScrollBarThickness = 4
    TCSContent.CanvasSize = UDim2.new(0, 0, 0, 200)
    TCSContent.Visible = true
    
    local TCSTitle = Instance.new("TextLabel", TCSContent)
    TCSTitle.Size = UDim2.new(1, -20, 0, 30)
    TCSTitle.Position = UDim2.new(0, 10, 0, 10)
    TCSTitle.BackgroundTransparency = 1
    TCSTitle.Text = "‚öΩ THE CLASSIC SOCCER"
    TCSTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    TCSTitle.Font = Enum.Font.GothamBlack
    TCSTitle.TextSize = 16
    
    -- MENSAGEM INFORMATIVA SOBRE A REMO√á√ÉO DOS CHEATS
    local WarningLabel = Instance.new("TextLabel", TCSContent)
    WarningLabel.Size = UDim2.new(0.9, 0, 0, 60)
    WarningLabel.Position = UDim2.new(0.05, 0, 0, 50)
    WarningLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    WarningLabel.Text = "‚ö†Ô∏è FUN√á√ïES REMOVIDAS\nOs cheats foram removidos por seguran√ßa\npara evitar banimento da conta."
    WarningLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    WarningLabel.Font = Enum.Font.GothamBold
    WarningLabel.TextSize = 12
    WarningLabel.TextWrapped = true
    
    -- Informa√ß√µes sobre o jogo
    local GameInfo = Instance.new("TextLabel", TCSContent)
    GameInfo.Size = UDim2.new(0.9, 0, 0, 80)
    GameInfo.Position = UDim2.new(0.05, 0, 0, 120)
    GameInfo.BackgroundColor3 = Color3.fromRGB(25, 25, 50)
    GameInfo.Text = "üìä INFORMA√á√ïES DO JOGO\n\n‚Ä¢ Jogo: The Classic Soccer\n‚Ä¢ Status: Online\n‚Ä¢ Vers√£o: Atualizada"
    GameInfo.TextColor3 = Color3.fromRGB(200, 200, 255)
    GameInfo.Font = Enum.Font.Gotham
    GameInfo.TextSize = 12
    GameInfo.TextWrapped = true
    
    -- ============================================================================
    -- 6. ABA ZARALHAR - COM OS CHEATS DO JEITO QUE VOC√ä QUER
    -- ============================================================================
    
    local ZaralharContent = Instance.new("ScrollingFrame", ContentArea)
    ZaralharContent.Size = UDim2.new(1, 0, 1, 0)
    ZaralharContent.BackgroundTransparency = 1
    ZaralharContent.ScrollBarThickness = 4
    ZaralharContent.CanvasSize = UDim2.new(0, 0, 0, 460) -- <<< SCROLL AJUSTADO AQUI (460)
    ZaralharContent.Visible = false
    
    local ZaralharTitle = Instance.new("TextLabel", ZaralharContent)
    ZaralharTitle.Size = UDim2.new(1, -20, 0, 30)
    ZaralharTitle.Position = UDim2.new(0, 10, 0, 10)
    ZaralharTitle.BackgroundTransparency = 1
    ZaralharTitle.Text = "üåÄ ZARALHAR CHEATS"
    ZaralharTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    ZaralharTitle.Font = Enum.Font.GothamBlack
    ZaralharTitle.TextSize = 16
    
    -- Vari√°veis globais
    local player = Players.LocalPlayer
    local flyActive = false
    local flySpeed = 100 -- Velocidade fixa alta pro fly
    local flyBodyVelocity = nil
    local flyConnection = nil
    local noclipActive = false
    local noclipConnection = nil
    local godModeActive = false
    local godModeConnection = nil
    
    -- Fun√ß√£o para criar bot√£o com status
    local function createCheatButton(name, description, yPosition)
        local buttonFrame = Instance.new("Frame", ZaralharContent)
        buttonFrame.Size = UDim2.new(0.9, 0, 0, 60)
        buttonFrame.Position = UDim2.new(0.05, 0, 0, yPosition)
        buttonFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        buttonFrame.BorderSizePixel = 0
        
        local button = Instance.new("TextButton", buttonFrame)
        button.Size = UDim2.new(0.4, 0, 1, 0)
        button.Position = UDim2.new(0, 0, 0, 0)
        button.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
        button.Text = "‚úÖ " .. name
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.GothamBold
        button.TextSize = 14
        
        local statusLabel = Instance.new("TextLabel", buttonFrame)
        statusLabel.Size = UDim2.new(0.25, 0, 1, 0)
        statusLabel.Position = UDim2.new(0.4, 0, 0, 0)
        statusLabel.BackgroundTransparency = 1
        statusLabel.Text = "INATIVO"
        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        statusLabel.Font = Enum.Font.GothamBold
        statusLabel.TextSize = 12
        
        local descLabel = Instance.new("TextLabel", buttonFrame)
        descLabel.Size = UDim2.new(0.35, 0, 1, 0)
        descLabel.Position = UDim2.new(0.65, 0, 0, 0)
        descLabel.BackgroundTransparency = 1
        descLabel.Text = description
        descLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextSize = 11
        descLabel.TextWrapped = true
        descLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        return button, buttonFrame, statusLabel
    end
    
    -- Fun√ß√£o para criar controle de +/- (DO JEITO QUE VOC√ä GOSTA)
    local function createSliderControl(name, yPos, minVal, maxVal, step, defaultVal, unit)
        local controlFrame = Instance.new("Frame", ZaralharContent)
        controlFrame.Size = UDim2.new(0.9, 0, 0, 60)
        controlFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        controlFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        controlFrame.BorderSizePixel = 0
        
        local nameLabel = Instance.new("TextLabel", controlFrame)
        nameLabel.Size = UDim2.new(0.4, 0, 0.5, 0)
        nameLabel.Position = UDim2.new(0, 0, 0, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = name
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextSize = 14
        
        local valueLabel = Instance.new("TextLabel", controlFrame)
        valueLabel.Size = UDim2.new(0.4, 0, 0.5, 0)
        valueLabel.Position = UDim2.new(0.4, 0, 0, 0)
        valueLabel.BackgroundTransparency = 1
        valueLabel.Text = defaultVal .. unit
        valueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        valueLabel.Font = Enum.Font.GothamBold
        valueLabel.TextSize = 14
        
        local buttonFrame = Instance.new("Frame", controlFrame)
        buttonFrame.Size = UDim2.new(0.2, 0, 0.5, 0)
        buttonFrame.Position = UDim2.new(0.8, 0, 0, 0)
        buttonFrame.BackgroundTransparency = 1
        
        local minusButton = Instance.new("TextButton", buttonFrame)
        minusButton.Size = UDim2.new(0.4, 0, 1, 0)
        minusButton.Position = UDim2.new(0, 0, 0, 0)
        minusButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        minusButton.Text = "-"
        minusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        minusButton.Font = Enum.Font.GothamBold
        minusButton.TextSize = 14
        
        local plusButton = Instance.new("TextButton", buttonFrame)
        plusButton.Size = UDim2.new(0.4, 0, 1, 0)
        plusButton.Position = UDim2.new(0.6, 0, 0, 0)
        plusButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        plusButton.Text = "+"
        plusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        plusButton.Font = Enum.Font.GothamBold
        plusButton.TextSize = 14
        
        local currentValue = defaultVal
        
        local function updateValue()
            valueLabel.Text = currentValue .. unit
            return currentValue
        end
        
        minusButton.MouseButton1Click:Connect(function()
            currentValue = math.max(minVal, currentValue - step)
            updateValue()
        end)
        
        plusButton.MouseButton1Click:Connect(function()
            currentValue = math.min(maxVal, currentValue + step)
            updateValue()
        end)
        
        -- Fun√ß√£o pra pegar o valor atual
        local function getValue()
            return currentValue
        end
        
        -- Fun√ß√£o pra definir valor
        local function setValue(val)
            currentValue = math.clamp(val, minVal, maxVal)
            updateValue()
        end
        
        -- Descri√ß√£o
        local descLabel = Instance.new("TextLabel", controlFrame)
        descLabel.Size = UDim2.new(1, 0, 0.5, 0)
        descLabel.Position = UDim2.new(0, 0, 0.5, 0)
        descLabel.BackgroundTransparency = 1
        descLabel.Text = "Ajuste de " .. step .. unit .. " em " .. step .. unit .. " (" .. minVal .. unit .. " a " .. maxVal .. unit .. ")"
        descLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextSize = 11
        descLabel.TextWrapped = true
        
        return controlFrame, getValue, setValue
    end
    
    -- 1. FLY (R√ÅPIDO E COM CONTROLE)
    local flyButton, flyFrame, flyStatus = createCheatButton("FLY", "Controles: WASD + E(Subir)/Q(Descer)", 50)
    
    flyButton.MouseButton1Click:Connect(function()
        flyActive = not flyActive
        
        if flyActive then
            -- Ativar fly
            flyButton.Text = "‚ùå FLY"
            flyButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            flyStatus.Text = "ATIVO"
            flyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            humanoid.PlatformStand = true
            
            -- Criar BodyVelocity
            flyBodyVelocity = Instance.new("BodyVelocity")
            flyBodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
            flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
            flyBodyVelocity.Parent = character.PrimaryPart
            
            flyConnection = RunService.Heartbeat:Connect(function()
                if character and character.PrimaryPart and flyActive then
                    local camera = workspace.CurrentCamera
                    local moveDirection = Vector3.new(0, 0, 0)
                    
                    -- Controles WASD
                    if UIS:IsKeyDown(Enum.KeyCode.W) then
                        moveDirection = moveDirection + camera.CFrame.LookVector
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.S) then
                        moveDirection = moveDirection - camera.CFrame.LookVector
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.D) then
                        moveDirection = moveDirection + camera.CFrame.RightVector
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.A) then
                        moveDirection = moveDirection - camera.CFrame.RightVector
                    end
                    
                    -- Controles E/Q
                    if UIS:IsKeyDown(Enum.KeyCode.E) then
                        moveDirection = moveDirection + Vector3.new(0, 1, 0)
                    end
                    if UIS:IsKeyDown(Enum.KeyCode.Q) then
                        moveDirection = moveDirection + Vector3.new(0, -1, 0)
                    end
                    
                    -- Aplicar velocidade (FIXA E ALTA)
                    if moveDirection.Magnitude > 0 then
                        moveDirection = moveDirection.Unit * flySpeed
                    end
                    
                    flyBodyVelocity.Velocity = moveDirection
                end
            end)
        else
            -- Desativar fly
            flyButton.Text = "‚úÖ FLY"
            flyButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            flyStatus.Text = "INATIVO"
            flyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if flyConnection then
                flyConnection:Disconnect()
                flyConnection = nil
            end
            
            local character = player.Character
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.PlatformStand = false
                end
                
                if flyBodyVelocity then
                    flyBodyVelocity:Destroy()
                    flyBodyVelocity = nil
                end
                
                character.PrimaryPart.Velocity = Vector3.new(0, 0, 0)
            end
        end
    end)
    
    -- FLY SPEED CONTROLE (NOVO - ADICIONADO DEPOIS DO FLY)
    local flySpeedControl, flySpeedGetValue, flySpeedSetValue = createSliderControl("FLY SPEED", 120, 10, 300, 10, 100, "")
    
    -- Atualizar velocidade do fly quando mudar
    local function updateFlySpeed()
        flySpeed = flySpeedGetValue()
    end
    
    -- Conectar os bot√µes do Fly Speed
    local flySpeedMinusButton = flySpeedControl:FindFirstChild("Frame"):FindFirstChild("TextButton")
    local flySpeedPlusButton = flySpeedControl:FindFirstChild("Frame"):FindFirstChild("TextButton", true)
    
    flySpeedMinusButton.MouseButton1Click:Connect(function()
        updateFlySpeed()
    end)
    
    flySpeedPlusButton.MouseButton1Click:Connect(function()
        updateFlySpeed()
    end)
    
    -- 2. WALKSPEED (CONTROLE DE 5 EM 5)
    local wsControl, wsGetValue, wsSetValue = createSliderControl("WALKSPEED", 190, 5, 200, 5, 16, "")
    
    -- Aplicar WalkSpeed quando mudar
    local function updateWalkSpeed()
        local speed = wsGetValue()
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = speed
            end
        end
    end
    
    -- Conectar os bot√µes +/-
    local wsMinusButton = wsControl:FindFirstChild("Frame"):FindFirstChild("TextButton")
    local wsPlusButton = wsControl:FindFirstChild("Frame"):FindFirstChild("TextButton", true)
    
    wsMinusButton.MouseButton1Click:Connect(function()
        updateWalkSpeed()
    end)
    
    wsPlusButton.MouseButton1Click:Connect(function()
        updateWalkSpeed()
    end)
    
    -- Aplicar inicial
    updateWalkSpeed()
    
    -- 3. JUMP POWER (CONTROLE DE 5 EM 5)
    local jpControl, jpGetValue, jpSetValue = createSliderControl("JUMP POWER", 260, 5, 300, 5, 50, "")
    
    -- Aplicar JumpPower quando mudar
    local function updateJumpPower()
        local power = jpGetValue()
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.JumpPower = power
            end
        end
    end
    
    local jpMinusButton = jpControl:FindFirstChild("Frame"):FindFirstChild("TextButton")
    local jpPlusButton = jpControl:FindFirstChild("Frame"):FindFirstChild("TextButton", true)
    
    jpMinusButton.MouseButton1Click:Connect(function()
        updateJumpPower()
    end)
    
    jpPlusButton.MouseButton1Click:Connect(function()
        updateJumpPower()
    end)
    
    -- Aplicar inicial
    updateJumpPower()
    
    -- 4. GOD MODE (CORRIGIDO DE VERDADE)
    local godButton, godFrame, godStatus = createCheatButton("GOD MODE", "Torna completamente invenc√≠vel", 330)
    
    godButton.MouseButton1Click:Connect(function()
        godModeActive = not godModeActive
        
        if godModeActive then
            godButton.Text = "‚ùå GOD MODE"
            godButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            godStatus.Text = "ATIVO"
            godStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")
            
            -- M√©todo SUPER eficaz
            local originalMaxHealth = humanoid.MaxHealth
            humanoid:SetAttribute("OriginalMaxHealth", originalMaxHealth)
            
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            
            -- Conectar para manter sa√∫de m√°xima
            godModeConnection = humanoid.HealthChanged:Connect(function()
                if humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                end
            end)
        else
            godButton.Text = "‚úÖ GOD MODE"
            godButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            godStatus.Text = "INATIVO"
            godStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if godModeConnection then
                godModeConnection:Disconnect()
                godModeConnection = nil
            end
            
            local character = player.Character
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    local originalMaxHealth = humanoid:GetAttribute("OriginalMaxHealth") or 100
                    humanoid.MaxHealth = originalMaxHealth
                    humanoid.Health = math.min(100, originalMaxHealth)
                end
            end
        end
    end)
    
    -- 5. NOCLIP (POSI√á√ÉO AJUSTADA)
    local noclipButton, noclipFrame, noclipStatus = createCheatButton("NOCLIP", "Atravessa paredes e objetos", 400)
    
    noclipButton.MouseButton1Click:Connect(function()
        noclipActive = not noclipActive
        
        if noclipActive then
            noclipButton.Text = "‚ùå NOCLIP"
            noclipButton.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
            noclipStatus.Text = "ATIVO"
            noclipStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            noclipConnection = RunService.Stepped:Connect(function()
                local character = player.Character
                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end
            end)
        else
            noclipButton.Text = "‚úÖ NOCLIP"
            noclipButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            noclipStatus.Text = "INATIVO"
            noclipStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
            
            if noclipConnection then
                noclipConnection:Disconnect()
                noclipConnection = nil
            end
            
            local character = player.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
        end
    end)
    
    -- Resetar quando personagem morre
    player.CharacterAdded:Connect(function()
        task.wait(0.5)
        
        if flyActive then
            flyActive = false
            if flyConnection then flyConnection:Disconnect() end
            if flyBodyVelocity then flyBodyVelocity:Destroy() end
            flyButton.Text = "‚úÖ FLY"
            flyButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            flyStatus.Text = "INATIVO"
            flyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        if godModeActive then
            godModeActive = false
            if godModeConnection then godModeConnection:Disconnect() end
            godButton.Text = "‚úÖ GOD MODE"
            godButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            godStatus.Text = "INATIVO"
            godStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        if noclipActive then
            noclipActive = false
            if noclipConnection then noclipConnection:Disconnect() end
            noclipButton.Text = "‚úÖ NOCLIP"
            noclipButton.BackgroundColor3 = Color3.fromRGB(60, 60, 150)
            noclipStatus.Text = "INATIVO"
            noclipStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
        
        -- Reaplicar WS e JP
        updateWalkSpeed()
        updateJumpPower()
    end)
    
    -- ============================================================================
    -- 7. BOT√ïES DAS ABAS
    -- ============================================================================
    
    local tabs = {}
    local tabContents = {}
    
    -- Bot√£o TCS (ativo por padr√£o)
    tabs.TCS = Instance.new("TextButton", TabPanel)
    tabs.TCS.Size = UDim2.new(1, -10, 0, 35)
    tabs.TCS.Position = UDim2.new(0, 5, 0, 20)
    tabs.TCS.BackgroundColor3 = Color3.fromRGB(128, 0, 32)
    tabs.TCS.Text = "‚öΩ TCS"
    tabs.TCS.TextColor3 = Color3.fromRGB(255, 255, 255)
    tabs.TCS.Font = Enum.Font.GothamBold
    
    -- Bot√µes das outras abas
    local otherTabs = {
        {"FPS", "üéØ", 70},
        {"ZARALHAR", "üåÄ", 120},
        {"UTILITARIOS", "‚öôÔ∏è", 170}
    }
    
    for _, tab in ipairs(otherTabs) do
        tabs[tab[1]] = Instance.new("TextButton", TabPanel)
        tabs[tab[1]].Size = UDim2.new(1, -10, 0, 35)
        tabs[tab[1]].Position = UDim2.new(0, 5, 0, tab[3])
        tabs[tab[1]].BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        tabs[tab[1]].Text = tab[2] .. " " .. tab[1]
        tabs[tab[1]].TextColor3 = Color3.fromRGB(255, 255, 255)
        tabs[tab[1]].Font = Enum.Font.GothamBold
    end
    
    -- ============================================================================
    -- 8. SISTEMA PARA TROCAR DE ABA
    -- ============================================================================
    
    -- Armazenar conte√∫dos das abas
    tabContents.TCS = TCSContent
    
    for _, tab in ipairs(otherTabs) do
        if tab[1] == "ZARALHAR" then
            tabContents[tab[1]] = ZaralharContent
        else
            local tabContent = Instance.new("Frame", ContentArea)
            tabContent.Size = UDim2.new(1, 0, 1, 0)
            tabContent.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
            tabContent.Visible = false
            
            local label = Instance.new("TextLabel", tabContent)
            label.Size = UDim2.new(1, 0, 1, 0)
            label.BackgroundTransparency = 1
            label.Text = tab[2] .. " " .. tab[1] .. "\n\n(Funcionalidades em desenvolvimento)"
            label.TextColor3 = Color3.fromRGB(200, 200, 200)
            label.Font = Enum.Font.Gotham
            label.TextSize = 16
            label.TextWrapped = true
            
            tabContents[tab[1]] = tabContent
        end
    end
    
    local function switchTab(tabName)
        for _, content in pairs(tabContents) do
            content.Visible = false
        end
        
        for name, btn in pairs(tabs) do
            btn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        end
        
        if tabContents[tabName] then
            tabContents[tabName].Visible = true
            tabs[tabName].BackgroundColor3 = Color3.fromRGB(128, 0, 32)
        end
    end
    
    for name, btn in pairs(tabs) do
        btn.MouseButton1Click:Connect(function()
            switchTab(name)
        end)
    end
    
    -- ============================================================================
    -- 9. FUNCIONALIDADES EXTRAS
    -- ============================================================================
    
    UIS.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.F5 then
            MainFrame.Visible = not MainFrame.Visible
        end
    end)
    
    local dragging = false
    local dragStart, startPos
    
    MainTitle.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
        end
    end)
    
    MainTitle.InputEnded:Connect(function()
        dragging = false
    end)
    
    UIS.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            MainFrame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
        end
    end)
    
    print("========================================")
    print("üéÆ NOEL TOXIC SUITE - VERS√ÉO PERFEITA")
    print("‚úÖ Bot√£o X adicionado (fecha o menu)")
    print("‚úÖ Fly Speed adicionado (10 a 300, de 10 em 10)")
    print("‚úÖ Scroll ajustado (CanvasSize = 460)")
    print("üåÄ ZARALHAR: Cheats completos:")
    print("   ‚Ä¢ FLY: Com controle de velocidade")
    print("   ‚Ä¢ FLY SPEED: Ajuste de 10 em 10")
    print("   ‚Ä¢ WALKSPEED: Controle de 5 em 5")
    print("   ‚Ä¢ JUMP POWER: Controle de 5 em 5")
    print("   ‚Ä¢ GOD MODE: Imortalidade total")
    print("   ‚Ä¢ NOCLIP: Funcionando perfeitamente")
    print("‚úÖ NADA MAIS FOI MEXIDO")
    print("========================================")
end

print("üéÆ NOEL TOXIC SUITE CARREGADO")
print("üîí Vers√£o perfeita - Completamente divina")
print("üñ±Ô∏è Pressione F5 para mostrar/esconder")
print("‚ùå Bot√£o X no canto superior direito")
